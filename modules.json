{
    "booter": {
        "alias": "引导程序",
        "modules": ["core", "crawler", "renderer"]
    },
    "core": {
        "alias": "核心",
        "modules": ["bot", "speaker"]
    },
    "bot": {
        "alias": "大语言模型",
        "kinds": ["basic", "dense", "sparse"],
        "default": "sparse",
        "modules": ["caller", "searcher"]
    },
    "caller": {
        "alias": "大语言模型调用器",
        "path": "bot",
        "kinds": ["gpt", "virtual"],
        "default": "gpt"
    },
    "searcher": {
        "alias": "文本检索器",
        "path": "bot",
        "kinds": ["es"],
        "default": "null",
        "notNull": false
    },
    "speaker": {
        "alias": "文本转语音",
        "kinds": ["basic", "gsv", "bv2"],
        "default": "bv2"
    },
    "crawler": {
        "alias": "直播间爬虫",
        "kinds": ["bilibili", "virtual"],
        "default": "virtual"
    },
    "renderer": {
        "alias": "人物渲染器",
        "kinds": ["easyaivtuber", "sound"],
        "default": "sound",
        "notNull": false
    }
}
